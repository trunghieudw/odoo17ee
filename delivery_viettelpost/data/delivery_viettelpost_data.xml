<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_product_delivery_viettelpost" model="product.product">
            <field name="name">ViettelPost Delivery Service</field>
            <field name="default_code">ViettelPost_001</field>
            <field name="type">service</field>
            <field name="categ_id" ref="delivery.product_category_deliveries"/>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
            <field name="invoice_policy">order</field>
        </record>
        <record id="delivery_carrier_viettelpost_default" model="delivery.carrier">
            <field name="name">ViettelPost</field>
            <field name="product_id" ref="delivery_viettelpost.product_product_delivery_viettelpost"/>
            <field name="delivery_type">viettelpost</field>
            <field name="country_ids" eval="[(4,(ref('base.vn')))]"/>
        </record>
    </data>
</odoo>
